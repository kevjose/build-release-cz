{
  "name": "with-tailwindcss",
  "version": "1.1.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "commit": "cz"
  },
  "dependencies": {
    "husky": "^4.3.8",
    "next": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "autoprefixer": "10.0.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-with-jiraid-detection": "^1.0.3",
    "postcss": "8.1.7",
    "tailwindcss": "^2.0.2"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-with-jiraid-detection"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}